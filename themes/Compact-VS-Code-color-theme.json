{
	"name": "Compact-VS-Code",	
	"colors": {
		"editor.background": "#191521",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb",
		"selection.background": "#3d375e7f",
	},
	"tokenColors": [
  {
    "scope": ["comment", "punctuation.definition.comment"],
    "settings": { "foreground": "#5A5A5A", "fontStyle": "italic" }
  },
  {
    "scope": ["keyword", "storage.type", "storage.modifier"],
    "settings": { "foreground": "#FF2EFF" } // neon magenta
  },
  {
    "scope": ["string", "constant.other.symbol"],
    "settings": { "foreground": "#7CFF4A" } // acid green
  },
  {
    "scope": ["entity.name.function", "support.function", "meta.function-call"],
    "settings": { "foreground": "#00E8FF" } // neon cyan
  },
  {
    "scope": ["variable", "identifier"],
    "settings": { "foreground": "#D0D0D0" } // light gray
  },
  {
    "scope": ["constant.numeric", "constant.language.boolean"],
    "settings": { "foreground": "#FF8A00" } // neon orange
  },
  {
    "scope": ["entity.name.type", "support.type", "entity.other.inherited-class"],
    "settings": { "foreground": "#a968e2" } // violet
  },
  {
    "scope": ["constant", "constant.character", "constant.other"],
    "settings": { "foreground": "#F6FF00" } // electric yellow
  },
  {
    "scope": ["punctuation", "punctuation.definition", "punctuation.separator"],
    "settings": { "foreground": "#A0A0A0" } // deep gray
  },
  {
    "scope": ["invalid", "invalid.illegal"],
    "settings": { "foreground": "#FF003C" } // neon red
  }
]


}
