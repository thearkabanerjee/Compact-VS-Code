  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5C6A7D"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#8BC34A"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language"
      ],
      "settings": {
        "foreground": "#FFCA28"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#FF5252"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function",
        "entity.name.method"
      ],
      "settings": {
        "foreground": "#00BCD4"
      }
    },
    {
      "scope": [
        "variable",
        "variable.parameter",
        "variable.declaration",
        "variable.other"
      ],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#78909C"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#5DADE2"
      }
    },
    {
      "scope": ["support.property", "meta.object-literal.key"],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#5DADE2"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCA28"
      }
    },
    {
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00BCD4"
      }
    },
    {
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#5DADE2"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ABB2BF"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ABB2BF"
      }
    },
    {
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#FFCA28"
      }
    },
    {
      "scope": ["string.quoted.double.json"],
      "settings": {
        "foreground": "#8BC34A"
      }
    }
  ]
}
